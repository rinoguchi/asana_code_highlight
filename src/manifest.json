{
  "name": "asana highlighter",
  "description": "highlight the source code block on Asana pages",
  "version": "1.0.1",
  "manifest_version": 3,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://app.asana.com/*"],
      "js": ["highlighter.js"]
    }
  ]
}
